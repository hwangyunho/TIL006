-- 1번
SELECT *
FROM EMP;

-- 2번
SELECT EMPNO, ENAME, SAL
FROM EMP;

-- 3번
SELECT DISTINCT SAL
FROM EMP;

-- 4번
SELECT ENAME AS '이 름', SAL AS '월 급' 
FROM EMP;

-- 5번
SELECT ENAME, SAL, SAL + ifnull(COMM, 0) AS 실급여
FROM EMP;

-- 6번
SELECT EMPNO, ENAME, SAL
FROM EMP
WHERE (ENAME = 'SCOTT');
 
 -- 7번
SELECT EMPNO, ENAME, JOB
FROM EMP
WHERE (JOB = 'SALESMAN');

-- 8번
SELECT EMPNO, ENAME, SAL
FROM EMP
where (EMPNO = '7499') OR (EMPNO = '7521') OR (EMPNO = '7654');

-- 9번 
SELECT EMPNO, ENAME, SAL
FROM EMP
WHERE SAL BETWEEN 1500 AND 3000;

-- 10번
SELECT ENAME
FROM EMP
WHERE ENAME LIKE 'A%N';